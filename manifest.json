{
  "manifest_version": 2,
  "name": "pomoApp",
  "version": "1.0",
  "developer": {
    "name": "Bar Sahar",
    "url": "https://github.com/barsahar"
  },
  "description": "pomodoro app with marked sites for focused mode",
  "icons": {
    "48": "icons/border-48.png",
    "96": "icons/border-48.png"
  },
  "permissions": [
    "alarms"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/border-48.png",
      "32": "icons/border-48.png",
      "64": "icons/border-48.png"
    },
    "default_title": "pomoApp",
    "default_popup": "popup/popup.html"
  }
}